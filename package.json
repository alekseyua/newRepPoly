{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "prettier": "prettier  -write ./src/**/*.js",
    "cypress:open": "cypress open",
    "test": "NODE_ENV=test razzle test --env=jsdom --no-cache",
    "build": "sh -ac '. config/.env.${REACT_APP_ENV}; razzle build'",
    "server": "sh -ac '. config/.env.${REACT_APP_ENV}; env NODE_ENV=server node build/server.js'",
    "build:staging": "REACT_APP_ENV=staging yarn build",
    "build:production": "REACT_APP_ENV=production yarn build",
    "server:staging": "REACT_APP_ENV=server.staging yarn server",
    "server:production": "REACT_APP_ENV=server.production yarn server"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/__tests__/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@garpix)"
    ]
  },
  "dependencies": {
    "@garpix/base-api": "./vendor/garpix-base-api-3.0.2.tgz",
    "@garpix/fetcher": "^1.0.4",
    "@garpix/garpix-web-components-react": "0.7.1",
    "@garpix/generator": "^0.1.3",
    "@storeon/localstorage": "^0.14.0",
    "@tiptap/react": "^2.0.0-beta.50",
    "@tiptap/starter-kit": "^2.0.0-beta.80",
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.8.25",
    "express": "^4.17.1",
    "formik": "^2.0.8",
    "framer-motion": "^6.0.0",
    "global": "^4.4.0",
    "jodit-react": "^1.1.1",
    "qs": "^6.9.6",
    "razzle": "3.3.2",
    "razzle-plugin-scss": "^3.3.2",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.0",
    "react-date-picker": "^8.1.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.3.2",
    "react-google-recaptcha-v3": "^1.9.4",
    "react-helmet": "^5.2.1",
    "react-icomoon": "^2.4.3",
    "react-id-swiper": "^4.0.0",
    "react-intl": "^5.6.8",
    "react-notifications-component": "^3.4.1",
    "react-paginating": "^1.4.0",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.25.2",
    "storeon": "2.0.2",
    "swiper": "6.5.8",
    "uuid": "^8.3.2",
    "video-react": "^0.14.1",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "prettier": "^2.2.1"
  }
}
